{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "css": [ "vk-download_styles.css" ],
      "js": [ "jquery_min.js", "contentscript.js" ],
      "matches": [ "https://vk.com/*", "http://vk.com/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": false,
      "js": [ "analytics.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Modifies pages with audio(eg. 'My music', 'Suggested music', 'Popular' etc) so you can download files directly from the web page.",
   "icons": {
      "128": "download128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "VK Music Downloader",
   "permissions": [ "https://vk.com/*", "http://vk.com/*" ],
   "short_name": "VK Music Downloader",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3",
   "web_accessible_resources": [ "download-icon.png" ]
}
